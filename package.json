{
  "name": "neo4j-graphql-js",
  "version": "0.1.18",
  "description": "A GraphQL to Cypher query execution layer for Neo4j. ",
  "main": "./dist/index.js",
  "scripts": {
    "start": "nodemon ./example/graphql-tools/movies.js --exec babel-node -e js",
    "build": "babel src --presets babel-preset-env --out-dir dist",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "ava test/cypherTest.js test/augmentSchemaTest.js --verbose"
  },
  "author": "William Lyon",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo4j-graphql/neo4j-graphql-js"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.11.1",
    "express": "^4.16.3",
    "apollo-server-express": "^1.3.6",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.0",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link-http": "^1.5.3",
    "body-parser": "^1.18.2",
    "node-fetch": "^2.1.2"
  },
  "dependencies": {
    "graphql": "^0.13.2",
    "lodash": "^4.17.10",
    "neo4j-driver": "^1.6.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
